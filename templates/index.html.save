                ['Mehedinți', 44.5515, 22.9044],
                ['Mureș', 46.5570, 24.6723],
                ['Neamț', 46.9759, 26.3819],
                ['Olt', 44.2008, 24.5023],
                ['Prahova', 45.0892, 26.0829],
                ['Satu Mare', 47.8017, 22.8576],
                ['Sălaj', 47.2091, 23.2122],
                ['Sibiu', 45.9269, 24.2255],
                ['Suceava', 47.5506, 25.7411],
                ['Teleorman', 44.0160, 25.2987],
                ['Timiș', 45.8139, 21.3331],
                ['Tulcea', 45.0451, 29.0325],
                ['Vaslui', 46.4631, 27.7398],
                ['Vâlcea', 45.0798, 24.0835],
                ['Vrancea', 45.8135, 27.0658],
            ];
            const Romania = {
                lat: 45.9432,
                lng: 24.9668
